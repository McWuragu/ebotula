#: src/messages.c:25
msgid ""
"\n"
"\tOptions:\n"
msgstr ""
"\n"
"\tOptions:\n"

#: src/messages.c:26
msgid "\t-a days\t\tSet the time in days for the automatic logoff.\n"
msgstr "\t-a Tage\t\tSetzt die Zeit in Tage für das automatische Abmelden.\n"

#: src/messages.c:27
msgid "\t-b name\t\tSet the name of the bot.\n"
msgstr "\t-b Name\t\tSetzt den Namen des Bots im.\n"

#: src/messages.c:28
msgid "\t-c sec\t\tSet the ping timeout of the network connection.\n"
msgstr "\t-c Sek\t\tSetzt das Pingtimout in Sekunden für die Netzwerkverbindung.\n"

#: src/messages.c:29
msgid "\t-d path\t\tSet the path of the database direcotry\n"
msgstr "\t-d Pfad\t\tSetzt den Pfad zur Datenbank.\n"

#: src/messages.c:30
msgid "\t-D Level\tSet the log level (0-7)\n"
msgstr "\t-D Level\tSetzt das Ausgabelevel (0-7)\n"

#: src/messages.c:31
msgid "\t-e ms\t\tSet the delay time in millisecond for the slow sending.\n"
msgstr ""
"\t-e ms\t\tSetzt die Verzögerung in Millisekunden für das verlangsamte "
"Senden.\n"

#: src/messages.c:32
msgid "\t-f file\t\tSet the config file.\n"
msgstr "\t-f Datei\tSetzt den Pfad zu Konfigurationsdatei.\n"

#: src/messages.c:33
msgid "\t-g group\tSet the group under wich the bot run (only root).\n"
msgstr ""
"\t-g Gruppe\tSetzt die Gruppe, unter der der Bot ausgeführt wird. (only "
"root).\n"

#: src/messages.c:34
msgid "\t-h\t\tPrint this help information.\n"
msgstr "\t-h\t\tGibt diese Hilfe aus.\n"

#: src/messages.c:35
msgid "\t-i sec\t\tSet the delay of the channel initialization.\n"
msgstr "\t-i Sek\t\tSetzt die Verzögerung in Sekunden für das Einrichten der Channels.\n"

#: src/messages.c:36
msgid "\t-k days\t\tSet the account live time in days.\n"
msgstr "\t-k Tage\t\tSetzt die Zeit in Tagen, die ein Account inaktiv sein kann.\n"

#: src/messages.c:37
msgid "\t-l char/min\tSet the charakter limit for the first sending delay level.\n"
msgstr "\t-l Char/min\tSetzt das Limit in Zeichen je Minute für das beschleunigte Senden.\n"

#: src/messages.c:38
msgid "\t-m\t\tStart the dialog for creating of a new master.\n"
msgstr "\t-m\t\tStartet den Dialog zum Anlegen eines neuen Masters.\n"

#: src/messages.c:39
msgid "\t-n ms\t\tSet the delay time in millisecond for the fast sending.\n"
msgstr ""
"\t-n ms\t\tSetzt die Verzögerung in Millisekunden für das beschleunigte "
"Senden\n"

#: src/messages.c:40
msgid "\t-p port\t\tSet the port of the irc server.\n"
msgstr "\t-p Port\t\tSetzt den Netzwerkport des IRC-Servers.\n"

#: src/messages.c:41
msgid "\t-r string\tSet the output for a request of the real name.\n"
msgstr "\t-r String\tSetzt die Zeichenkette des Real Name Fields für das IRC.\n"

#: src/messages.c:42
msgid "\t-s server\tSet the irc server name.\n"
msgstr "\t-s Server\tSetzt den Namen des IRC-Servers.\n"

#: src/messages.c:43
msgid "\t-t count\tSet the number of created  threads.\n"
msgstr "\t-t Anzahl\tSetzt die Anzahl der zu erzeugenden Threads.\n"

#: src/messages.c:44
msgid "\t-u user\t\tSet the user under which the bot run (root only).\n"
msgstr "\t-u Benutzer\tSetzt den Benutzer, unter dem der Bot lauft. (root only).\n"

#: src/messages.c:45
msgid "\t-v\t\tPrint version.\n"
msgstr "\t-v\t\tGibt die Version aus.\n"

